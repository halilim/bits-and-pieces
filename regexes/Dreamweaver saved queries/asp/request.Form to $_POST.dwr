<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="request.Form\(&quot;([^&quot;]*)&quot;\)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$_POST[&quot;$1&quot;]" param2=""/>
</dwquery>
